.image-box
    background: #F8F8F8
    border: 5px dashed #DDD
    width: 200px
    height: 200px
    text-align: center
    padding-top: 25px
    margin: 10px

.icon
  margin:15px
  margin-left: auto
  margin-right: auto
  width:100%
  height:200px
  background-color:#F8F8F8
  border:5px dashed #DDD
  position:relative
  border-radius:15px

.camera2
  display:block
  width:100px
  height:60px
  position:absolute
  top:35%
  left:40%
  background-color:#DDD
  border-radius:5px

.camera2:after
  content:""
  display:block
  width:35px
  height:35px
  border:7px solid #F8F8F8
  position:absolute
  top:20%
  left:40%
  background-color:#DDD
  border-radius:30px

.camera2:before
  content:""
  display:block
  width:100%
  height:7px
  position:absolute
  top:30%
  left:0%
  background-color:#F8F8F8

.camera2 span
  display:block
  width:30px
  height:20px
  position:absolute
  top:-15%
  left:40%
  background-color:#DDD

.camera2 span:after
  display:block
  content:""
  width:15px
  height:5px
  position:absolute
  top:15%
  left:25%
  background-color:#F8F8F8

.camera2 span:before
  display:block
  content:""
  width:15px
  height:5px
  position:absolute
  top:25%
  left:-90%
  background-color:#DDD

